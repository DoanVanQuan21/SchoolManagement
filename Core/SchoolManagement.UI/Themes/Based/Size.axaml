<ResourceDictionary
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:sys="clr-namespace:System;assembly=mscorlib">

    <sys:Double x:Key="DefaultControlHeight">35</sys:Double>
    <sys:Double x:Key="DefaultFontSize">18</sys:Double>
    <sys:Double x:Key="SmallControlHeight">20</sys:Double>
    <Thickness x:Key="DefaultControlPadding">10,5</Thickness>
    <Thickness x:Key="DefaultLabelControlMargin">0,10,0,10</Thickness>
    <Thickness x:Key="DefaultControlMargin">10,10,10,10</Thickness>
    <Thickness x:Key="DefaultInputPadding">10,0</Thickness>
    <CornerRadius x:Key="DefaultCornerRadius">10</CornerRadius>
</ResourceDictionary>
